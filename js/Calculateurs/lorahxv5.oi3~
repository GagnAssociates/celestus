//Fonctions de calcul
function calculCoutModules() {
    SaveToLocal();
    var bat = $('#selectBatiment').val();
    if (bat && typeof listeBats[bat] !== 'undefined') {
        var met = listeBats[bat].coutM;
        var trit = listeBats[bat].coutT;
        var pp = listeBats[bat].coutP;
        var actuel = $('#nudActuel').val();
        var voulu = $('#nudDesire').val();
        var mult = listeBats[bat].mult;
        var spe = $('#nudReduc').val();
        var subvention = $('#nudPolitiqueBat').val();
        var isBuildable = listeBats[bat].mkII;
        var gouv = $('#chkGouvBat').is(':checked');
        var tauxMet = $('#tauxMetal').NumBox('getRaw') / 1000000;
        var tauxTrit = $('#tauxTritium').NumBox('getRaw') / 1000000;
        var tauxPP = $('#tauxPP').NumBox('getRaw');
        var scl = $('#chkSCL').is(':checked');

        var coutMetal = coutMetTritBat(met, actuel, voulu, mult, spe, gouv, 0, scl);
        $('#coutMetal').text(big_number(coutMetal)).attr('title', formatNumber(coutMetal));

        var coutTrit = coutMetTritBat(trit, actuel, voulu, mult, spe, gouv, 0, scl);
        $('#coutTrit').text(big_number(coutTrit)).attr('title', formatNumber(coutTrit));

        var coutPP = coutPPBat(pp, actuel, voulu, gouv, scl);
        $('#coutPP').text(big_number(coutPP)).attr('title', formatNumber(coutPP));

        var coutZRC = coutZRCItem(coutMetal, coutTrit, coutPP, tauxMet, tauxTrit, tauxPP);
        $('#coutZRC').text(big_number(coutZRC)).attr('title', formatNumber(coutZRC));



        var coutReelMetal = coutMetTritBat(met, actuel, voulu, mult, spe, gouv, subvention, scl);
        $('#coutReelMetal').text(big_number(coutReelMetal)).attr('title', formatNumber(coutReelMetal));

        var coutReelTrit = coutMetTritBat(trit, actuel, voulu, mult, spe, gouv, subvention, scl);
        $('#coutReelTrit').text(big_number(coutReelTrit)).attr('title', formatNumber(coutReelTrit));

        var coutReelPP = coutPPBat(pp, actuel, voulu, gouv, scl);
        $('#coutReelPP').text(big_number(coutReelPP)).attr('title', formatNumber(coutReelPP));

        var coutReelZRC = coutZRCItem(coutReelMetal, coutReelTrit, coutReelPP, tauxMet, tauxTrit, tauxPP);
        $('#coutReelZRC').text(big_number(coutReelZRC)).attr('title', formatNumber(coutReelZRC));

        var coutMKII = isBuildable ? nbrModMKII(met, actuel, voulu, mult) : 0;
        //if (isBuildable) {
        $('#txtNbrMKII').NumBox('setRaw', coutMKII);
        $('#nbrMKII').text(formatNumber(coutMKII)).attr('title', big_number(coutMKII));
        $('#nbrReelMKII').text(formatNumber(coutMKII)).attr('title', big_number(coutMKII));
        calcBatisseur(null, coutMKII);
        calcBatisseur(null, coutMKII);
        //}

        var coutPPMKII = isBuildable ? Math.floor(coutPP * 0.75) : 0;
        $('#coutPPMKII').text(big_number(coutPPMKII)).attr('title', formatNumber(coutPPMKII));
        $('#coutReelPPMKII').text(big_number(coutPPMKII)).attr('title', formatNumber(coutPPMKII));
    }
}

function calculCoutTechnologie() {
    SaveToLocal();
    var tech = $('#selectTechnologie').val();
    if (tech && typeof listeTechs[tech] !== 'undefined') {
        var met = listeTechs[tech].coutM;
        var trit = listeTechs[tech].coutT;
        var pp = listeTechs[tech].coutP;
        var actuel = $('#nudActuelTech').val();
        var voulu = $('#nudDesireTech').val();
        var donnees = $('#nudObjReduc').val();
        var division = $('#nudPtsTech').val();
        var subvention = $('#nudPolitiqueTech').val();
        var mult = listeTechs[tech].mult;
        var spe = $('#nudReducTech').val();
        var gouv = $('#chkGouvTech').is(':checked');
        var CQ = $('#chkCQActive').is(':checked') ? 2 : 1;
        var tauxMet = $('#tauxMetal').NumBox('getRaw') / 1000000;
        var tauxTrit = $('#tauxTritium').NumBox('getRaw') / 1000000;
        var tauxPP = $('#tauxPP').NumBox('getRaw');

        var coutMetal = coutMetTritTech(met, actuel, voulu, mult, spe, gouv, donnees, divTech(division), CQ, 0);
        $('#coutMetalTech').text(big_number(coutMetal)).attr('title', formatNumber(coutMetal));

        var coutTrit = coutMetTritTech(trit, actuel, voulu, mult, spe, gouv, donnees, divTech(division), CQ, 0);
        $('#coutTritTech').text(big_number(coutTrit)).attr('title', formatNumber(coutTrit));

        var coutPP = coutPPTech(pp, actuel, voulu, gouv, divTech(division), CQ, 0);
        $('#coutPPTech').text(big_number(coutPP)).attr('title', formatNumber(coutPP));


        var coutZRC = coutZRCItem(coutMetal, coutTrit, coutPP, tauxMet, tauxTrit, tauxPP);
        $('#coutZRCTech').text(big_number(coutZRC)).attr('title', formatNumber(coutZRC));

        var coutReelMetal = coutMetTritTech(met, actuel, voulu, mult, spe, gouv, donnees, divTech(division), CQ, subvention);
        $('#coutReelMetalTech').text(big_number(coutReelMetal)).attr('title', formatNumber(coutReelMetal));

        var coutReelTrit = coutMetTritTech(trit, actuel, voulu, mult, spe, gouv, donnees, divTech(division), CQ, subvention);
        $('#coutReelTritTech').text(big_number(coutReelTrit)).attr('title', formatNumber(coutReelTrit));

        var coutReelPP = coutPPTech(pp, actuel, voulu, gouv, divTech(division), CQ, subvention);
        $('#coutReelPPTech').text(big_number(coutReelPP)).attr('title', formatNumber(coutReelPP));


        var coutReelZRC = coutZRCItem(coutReelMetal, coutReelTrit, coutReelPP, tauxMet, tauxTrit, tauxPP);
        $('#coutReelZRCTech').text(big_number(coutReelZRC)).attr('title', formatNumber(coutReelZRC));

        $('#nudPtsTech').attr('title', 'Réduction: ' + divTech(division));

        var totalZRCTech = Math.floor((coutMetTritTech(met, actuel, voulu, mult, 0, false, 0, 1, 1, 0) * tauxMet + coutMetTritTech(trit, actuel, voulu, mult, 0, false, 0, 1, 1, 0) * tauxTrit + coutPPTech(pp, actuel, voulu, false, 1, 1) * tauxPP) / 10);
        $('#totalZRCTech').text(big_number(totalZRCTech)).attr('title', formatNumber(totalZRCTech));
        $('#totalReelZRCTech').text(big_number(totalZRCTech)).attr('title', formatNumber(totalZRCTech));
        var economieZRC = Math.floor(coutZRC - totalZRCTech);
        $('#totalEconomieZRCTech').text(big_number(economieZRC)).attr('title', formatNumber(economieZRC));
        if (economieZRC > 0) {
            $('#totalEconomieZRCTech').parent('div').removeClass('bg-danger');
            $('#totalEconomieZRCTech').parent('div').addClass('bg-success');
        } else {
            $('#totalEconomieZRCTech').parent('div').removeClass('bg-success');
            $('#totalEconomieZRCTech').parent('div').addClass('bg-danger');
        }
        var economieReelZRC = Math.floor(coutReelZRC - totalZRCTech);
        $('#totalReelEconomieZRCTech').text(big_number(economieReelZRC)).attr('title', formatNumber(economieReelZRC));
        if (economieReelZRC > 0) {
            $('#totalReelEconomieZRCTech').parent('div').removeClass('bg-danger');
            $('#totalReelEconomieZRCTech').parent('div').addClass('bg-success');
        } else {
            $('#totalReelEconomieZRCTech').parent('div').removeClass('bg-success');
            $('#totalReelEconomieZRCTech').parent('div').addClass('bg-danger');
        }
    }
}

function calculCoutVaisseaux()
{
    SaveToLocal();
    var vaisseau = $("#selectVaisseau").val();
    if (vaisseau && trouverVaisseau(vaisseau)) {
        var met = trouverVaisseau(vaisseau).coutM;
        var trit = trouverVaisseau(vaisseau).coutT;
        var pop = trouverVaisseau(vaisseau).coutPop
        var pp = trouverVaisseau(vaisseau).coutP;
        var zrc = trouverVaisseau(vaisseau).coutZ;
        var nbrVaiss = parseInt($('#txtNbrVaiss').NumBox('getRaw'));
        nbrVaiss = isNaN(nbrVaiss) ? 0 : nbrVaiss;
        var subvention = $('#nudPolitiqueVaiss').val();
        var temps = trouverVaisseau(vaisseau).temps;
        var spe = $('#nudReducVaiss').val();
        var gouv = $('#chkGouvAmi').is(':checked');
        var opti = $('#chkOptiPlan').is(':checked');
        var mc = $('#chkMetaCite').is(':checked');
        var bases = $('#nudNiveauBS').val();
        var chantier = $('#nudNiveauChantier').val();
        var uc = $('#nudNiveauUC').val();
        var tc = $('#nudNiveauTC').val();
        var coeffZetra = $("#selectCoeffFdf").val();
        var bonusSegment = $("#selectBonusSegment").val();
        var tauxMet = $('#tauxMetal').NumBox('getRaw') / 1000000;
        var tauxTrit = $('#tauxTritium').NumBox('getRaw') / 1000000;
        var tauxPP = $('#tauxPP').NumBox('getRaw');
        var scl = $("#chkStaLour").is(":checked");

        var coutVaisseau = coutVaisseaux(vaisseau, nbrVaiss, gouv, 0);
        var coutReelVaisseau = coutVaisseaux(vaisseau, nbrVaiss, gouv, subvention);

        var totalZRC = Math.floor(coutEquivZRC(coutVaisseau.metal, coutVaisseau.tritium, 0, coutVaisseau.zrc, tauxMet, tauxTrit, tauxPP) * 1.10);
        var totalReelZRC = Math.floor(coutEquivZRC(coutReelVaisseau.metal, coutReelVaisseau.tritium, 0, coutReelVaisseau.zrc, tauxMet, tauxTrit, tauxPP) * 1.10);

        $('#totalZRCVaiss').text(big_number(totalZRC)).attr('title', formatNumber(totalZRC));
        $('#coutMetalVaiss').text(big_number(coutVaisseau.metal)).attr('title', formatNumber(coutVaisseau.metal));
        $('#coutTritVaiss').text(big_number(coutVaisseau.tritium)).attr('title', formatNumber(coutVaisseau.tritium));
        $('#coutPopVaiss').text(big_number(coutVaisseau.pop)).attr('title', formatNumber(coutVaisseau.pop));
        $('#coutZRCVaiss').text(big_number(coutVaisseau.zrc)).attr('title', formatNumber(coutVaisseau.zrc));

        $('#totalZRCReelVaiss').text(big_number(totalReelZRC)).attr('title', formatNumber(totalReelZRC));
        $('#coutReelMetalVaiss').text(big_number(coutReelVaisseau.metal)).attr('title', formatNumber(coutReelVaisseau.metal));
        $('#coutReelTritVaiss').text(big_number(coutReelVaisseau.tritium)).attr('title', formatNumber(coutReelVaisseau.tritium));
        $('#coutReelPopVaiss').text(big_number(coutReelVaisseau.pop)).attr('title', formatNumber(coutReelVaisseau.pop));
        $('#coutReelZRCVaiss').text(big_number(coutReelVaisseau.zrc)).attr('title', formatNumber(coutReelVaisseau.zrc));

        var tempsConstruction = formatTime(tempsVaisseauxDefense(temps, nbrVaiss, bonusSegment, chantier, bases, uc, tc, mc, opti, coeffZetra, scl));
        var tempsReelConstruction = formatTime(tempsVaisseauxDefense(temps, nbrVaiss, bonusSegment, chantier, bases, uc, tc, mc, opti, coeffZetra, scl));

        $('#nbrTempsVaiss').text(tempsConstruction).attr('title', tempsConstruction);
        $('#nbrTempsReelVaiss').text(tempsReelConstruction).attr('title', tempsReelConstruction);
    }
}

function calcRampe(element) {
    var coutGiZ = $('#prixGiZ').NumBox('getRaw');
    SaveToLocal();
    var niveauRampe, porteeRampe;
    if (element && $(element).attr('id') === 'nudPorteeRampe') {
        porteeRampe = $('#nudPorteeRampe').val();
        niveauRampe = Math.ceil(Math.pow(porteeRampe / 100, 2));
        $('#nudNiveauRampe').val(niveauRampe);
    }
    else {
        niveauRampe = $('#nudNiveauRampe').val();
        $('#nudPorteeRampe').val(Math.floor(100 * Math.sqrt(niveauRampe) * 100) / 100);
    }
    $('#coutRampePP').text(big_number(12 * niveauRampe)).attr('title', formatNumber(12 * niveauRampe));
    $('#coutRampeMC').text(big_number(25 * niveauRampe)).attr('title', formatNumber(25 * niveauRampe));
    $('#prixRampeElements').text(big_number(niveauRampe * coutGiZ)).attr('title', formatNumber(niveauRampe * coutGiZ));
}

function calcAIA() {
    SaveToLocal();
    var niveauAIA = $('#nudNiveauAIA').val();
    var consoMetal = Math.floor(24 * 10000000 * Math.pow(niveauAIA, 0.5) * (10 / (Math.pow(niveauAIA, 0.75) + 10) + 0.25));
    var consoTritium = Math.floor(consoMetal / 2);
    var coeffAIA = Math.floor(10000 * (0.13 * (1 - 0.5 * Math.pow(0.95, niveauAIA)))) / 100;
    coeffAIA = niveauAIA === 0 ? 0 : coeffAIA;
    $('#absorptionMetal').text(big_number(consoMetal)).attr('title', formatNumber(consoMetal));
    $('#absorptionTritium').text(big_number(consoTritium)).attr('title', formatNumber(consoTritium));
    $('#bonusAIA').text(coeffAIA + "%").attr('title', coeffAIA + "%");

}

function divTech(points) {
    return Math.max(1, Math.round(100 * Math.pow(points / 100, 0.5)) / 100);
}

function trouverVaisseau(id)
{
    var vaissTrouve;
    $.each(listePlansVaisseaux, function (key, value) {
        if (value.codeVaisseau == id) {
            //console.log(value.codeVaisseau);
            vaissTrouve = value;
        }
    });
    return vaissTrouve;
}

function coutVaisseaux(idVaisseau, quantite, gouvAm, subvention) {
    var vaisseau = trouverVaisseau(idVaisseau);
    var reponse = [];
    reponse.metal = parseInt(quantite * vaisseau.coutM * (gouvAm ? 0.9 : 1) * (1 - subvention / 100));
    reponse.tritium = parseInt(quantite * vaisseau.coutT * (gouvAm ? 0.9 : 1) * (1 - subvention / 100));
    reponse.pop = parseInt(quantite * vaisseau.coutPop * (gouvAm ? 0.9 : 1) * (1 - subvention / 100));
    reponse.pp = parseInt(quantite * vaisseau.coutP * (gouvAm ? 0.9 : 1) * (1 - subvention / 100));
    reponse.zrc = parseInt(quantite * vaisseau.coutZ);

    return reponse;
}

function calcBatisseur(totalCoutZRCBat, nbrMKIIBat) {
    SaveToLocal();
    var gouvAm = $('#chkGouvAm').is(':checked');
    var optPlan = $('#chkOptPlan').is(':checked');
    var totalZRCBat = !totalCoutZRCBat ? parseInt(unFormat($('#coutZRC').attr('title'))) : parseInt(totalCoutZRCBat);
    var totalReelZRCBat = !totalCoutZRCBat ? parseInt(unFormat($('#coutReelZRC').attr('title'))) : parseInt(totalCoutZRCBat);
    var nbrMKII = !nbrMKIIBat ? parseInt(unFormat($('#nbrMKII').text())) : nbrMKIIBat;
    nbrMKII = (nbrMKII !== parseInt($('#txtNbrMKII').NumBox('getRaw'))) ? parseInt($('#txtNbrMKII').NumBox('getRaw')) : nbrMKII;
    nbrMKII = isNaN(nbrMKII) ? 0 : nbrMKII;
    var nbrChantiers = $('#nudNbrChan').val();
    var tauxMet = $('#tauxMetal').NumBox('getRaw') / 1000000;
    var tauxTrit = $('#tauxTritium').NumBox('getRaw') / 1000000;
    var tauxPP = $('#tauxPP').NumBox('getRaw');
    var subvention = $('#nudPolitiqueBatMKII').val();

    var coutModules = coutVaisseaux("VX15", nbrMKII, gouvAm, 0);
    var coutReelModules = coutVaisseaux("VX15", nbrMKII, gouvAm, subvention);

    //var coutMet = coutModules.metal;

    //var coutTrit = coutModules.tritium;

    //var coutZRC = coutModules.zrc;

    var totalZRC = Math.floor(coutEquivZRC(coutModules.metal, coutModules.tritium, 0, coutModules.zrc, tauxMet, tauxTrit, tauxPP) * 1.10);
    var totalReelZRC = Math.floor(coutEquivZRC(coutReelModules.metal, coutReelModules.tritium, 0, coutReelModules.zrc, tauxMet, tauxTrit, tauxPP) * 1.10);

    $('#totalZRCMKII').text(big_number(totalZRC)).attr('title', formatNumber(totalZRC));
    $('#coutMetalMKII').text(big_number(coutModules.metal)).attr('title', formatNumber(coutModules.metal));
    $('#coutTritMKII').text(big_number(coutModules.tritium)).attr('title', formatNumber(coutModules.tritium));
    $('#coutPopMKII').text(big_number(coutModules.pop)).attr('title', formatNumber(coutModules.pop));
    $('#coutZRCMKII').text(big_number(coutModules.zrc)).attr('title', formatNumber(coutModules.zrc));

    $('#totalReelZRCMKII').text(big_number(totalReelZRC)).attr('title', formatNumber(totalReelZRC));
    $('#coutReelMetalMKII').text(big_number(coutReelModules.metal)).attr('title', formatNumber(coutReelModules.metal));
    $('#coutReelTritMKII').text(big_number(coutReelModules.tritium)).attr('title', formatNumber(coutReelModules.tritium));
    $('#coutReelPopMKII').text(big_number(coutReelModules.pop)).attr('title', formatNumber(coutReelModules.pop));
    $('#coutReelZRCMKII').text(big_number(coutReelModules.zrc)).attr('title', formatNumber(coutReelModules.zrc));

    //var tempsTotal = Math.ceil(nbrMKII / ((nbrChantiers > nbrMKII) ? nbrMKII : nbrChantiers)) * ((optPlan) ? 19 : 24);
    var tempsTotal = Math.ceil(tempsVaisseauxDefense(trouverVaisseau("VX15").temps, nbrMKII, 1.4, 150, 125, 150, 150, 1, optPlan, 1, 0) / Math.max(1,Math.min(nbrChantiers,nbrMKII)));
    $('#nbrTempsMKII').text(formatTime(tempsTotal));
    $('#nbrReelTempsMKII').text(formatTime(tempsTotal));

    var economieZRC = Math.floor(totalZRCBat - totalZRC);
    var economieReelZRC = Math.floor(totalReelZRCBat - totalReelZRC);
    $('#totalEconomieZRC').text(big_number(economieZRC)).attr('title', formatNumber(economieZRC));
    if (economieZRC > 0) {
        $('#totalEconomieZRC').parent('div').removeClass('bg-danger');
        $('#totalEconomieZRC').parent('div').addClass('bg-success');
    } else {
        $('#totalEconomieZRC').parent('div').removeClass('bg-success');
        $('#totalEconomieZRC').parent('div').addClass('bg-danger');
    }
    $('#totalReelEconomieZRC').text(big_number(economieReelZRC)).attr('title', formatNumber(economieReelZRC));
    if (economieReelZRC > 0) {
        $('#totalReelEconomieZRC').parent('div').removeClass('bg-danger');
        $('#totalReelEconomieZRC').parent('div').addClass('bg-success');
    } else {
        $('#totalReelEconomieZRC').parent('div').removeClass('bg-success');
        $('#totalReelEconomieZRC').parent('div').addClass('bg-danger');
    }

    //alert(nbrMKII);
}

function coutZRCItem(met, trit, pp, tauxMet, tauxTrit, tauxPP) {
    return Math.floor((met * tauxMet) + (trit * tauxTrit) + (pp * tauxPP));
}

function coutEquivZRC(met, trit, pp, zrc, tauxMet, tauxTrit, tauxPP) {
    return Math.floor((met * tauxMet) + (trit * tauxTrit) + (pp * tauxPP) + parseInt(zrc));
}

function nbrModMKII(met, actuel, voulu, mult) {
    var totalMKII = 0;
    for (var niv = parseInt(actuel) ; niv < parseInt(voulu) ; niv++) {
        var nbrMKII = Math.ceil(0.85 * met * (Math.pow(mult, niv)) / 16000000, 0);
        totalMKII += nbrMKII;
    }
    return totalMKII;
}

function coutMetTritBat(res, actuel, voulu, mult, spe, gouv, subvention, scl) {
    if (scl)
    {
        actuel--;
        voulu--;
    }
    var totalRes = 0;
    for (var niv = parseInt(actuel) ; niv < parseInt(voulu) ; niv++) {
        if (gouv) {
            totalRes += res * Math.pow(mult, niv) * 0.9 * Math.pow(0.99, spe) * (1-subvention/100);
        } else {
            totalRes += res * Math.pow(mult, niv) * Math.pow(0.99, spe) * (1-subvention/100);
        }
    }
    return Math.floor(totalRes);

}

function coutMetTritTech(res, actuel, voulu, mult, spe, gouv, donnees, division, CQ, subvention) {
    var totalRes = 0;
    for (var niv = parseInt(actuel) ; niv < parseInt(voulu) ; niv++) {
        if (gouv) {
            totalRes += res * Math.pow(mult, niv) * 0.9 * Math.pow(0.99, spe) * ReducDonnees(donnees) / division / CQ * (1-subvention/100);
        } else {
            totalRes += res * Math.pow(mult, niv) * Math.pow(0.99, spe) * ReducDonnees(donnees) / division / CQ * (1-subvention/100);
        }
    }
    return Math.floor(totalRes);

}

function tempsVaisseauxDefense(tempsBase, nombre, bonusSegment, batCS, BS, batUC, TC, metaCite, competenceTemps, coeffGuilde, baseConstruction) {
    batCS = Math.max(batCS, Math.ceil(45*Math.sqrt(BS)));
    var divisionVaisseau = bonusSegment * Math.min(44000, 0.6 * batCS * Math.pow(1.01, batCS) * Math.pow(1.15, 1 * batUC + Math.sqrt(TC)) * Math.pow(10, Math.min(1, metaCite)) * Math.pow(10, Math.min(1, baseConstruction)));
    return Math.ceil(tempsBase * nombre * Math.pow(0.8, competenceTemps) / coeffGuilde / divisionVaisseau);
}

function ReducDonnees(nombre)
{
    nombre = Math.min(5, nombre);
    return 1 - (nombre * (11 - nombre) / 200);
}

function coutPPBat(pp, actuel, voulu, gouv, scl) {
    if (scl) {
        actuel--;
        voulu--;
    }
    var totalPP = 0;
    for (var niv = parseInt(actuel) ; niv < parseInt(voulu) ; niv++) {
        if (gouv) {
            totalPP += Math.round(pp * Math.pow(2, niv) * 0.9);
        } else {
            totalPP += pp * Math.pow(2, niv);
        }
    }
    return Math.floor(totalPP);

}

function coutPPTech(pp, actuel, voulu, gouv, division, CQ) {
    var totalPP = 0;
    for (var niv = parseInt(actuel) ; niv < parseInt(voulu) ; niv++) {
        if (gouv) {
            totalPP += Math.round(pp * Math.pow(1.7, niv) * 0.9) / division / CQ;
        } else {
            totalPP += pp * Math.pow(1.7, niv) / division / CQ;
        }
    }
    return Math.floor(totalPP);

}

function refroidAT(niveau) {
    var totalSecondes = (20 / Math.sqrt(niveau)) * 86400;

    return formatTime(totalSecondes);
}

function joursAT(niveau) {
    return 4 + niveau;
}

function AjouterBatimentListe() {
    AjouterBatListe(
        $('#selectBatiment').val(),
        $('#selectBatiment option:selected').text(),
        $('#txtColonieBat').val(),
        formatNumber($('#nudActuel').val()),
        formatNumber($('#nudDesire').val()),
        unFormat($('#coutMetal').attr('title')),
        unFormat($('#coutTrit').attr('title')),
        unFormat($('#coutPP').attr('title')),
        ((!$('#txtNbrMKII').NumBox('getRaw')) ? 0 : $('#txtNbrMKII').NumBox('getRaw'))
        );
}

function AjouterTechnologieListe() {
    AjouterTechListe(
        $('#selectTechnologie').val(),
        $('#selectTechnologie option:selected').text(),
        formatNumber($('#nudActuelTech').val()),
        formatNumber($('#nudDesireTech').val()),
        unFormat($('#coutMetalTech').attr('title')),
        unFormat($('#coutTritTech').attr('title')),
        unFormat($('#coutPPTech').attr('title')),
        unFormat($('#totalZRCTech').attr('title'))
        );
}

function AjouterBatListe(codeBat, nomBat, nomColo, nivActuel, nivVoulu, coutM, coutT, coutP, coutMKII) {
    var listeBats = $('#listeBatiments');
    $('#listeBatiments').bootstrapTable('append', {
        codeBat: codeBat, nomBat: nomBat, nomColo: nomColo, nivActuel: nivActuel, nivVoulu: nivVoulu, coutM: coutM, coutT: coutT, coutP: coutP, coutMKII: coutMKII, actions: "<input type='button' class='btn btn-danger' value='x' />"
    });

    listeBats.find('tbody td .btn').on('click', function () { EnleverBatimentListe(this); });

    CalcTotalBatimentsListe();
}

function AjouterVaisseauRecherche(codeVaiss, nomVaiss, chassis, coutM, coutT, coutP, coutZRC) {
    var listeRechVai = $("#listeRechercheVaisseaux");
    listeRechVai.bootstrapTable('append', {
        codeVaiss: codeVaiss, nomVaiss: nomVaiss, chassis: typesVaisseaux[chassis], coutM: coutM, coutT: coutT, coutP: coutP, coutZRC: coutZRC
    });
}

function RechercherPlansVaisseaux(nomVaiss, chassis) {
    console.log(nomVaiss, chassis);
    var vaisseauxTrouves = new Array();
    $.each(listePlansVaisseaux, function (e, $element) {
        var trouve = true;
        trouve = trouve * $element.nom.includes(nomVaiss);
        trouve = (chassis != "") ? ($element.chassis == chassis) * trouve : trouve;


        if (trouve) { vaisseauxTrouves.push($element);}
    });
    return vaisseauxTrouves;
}

function AjouterTechListe(codeTech, nomTech, nivActuel, nivVoulu, coutM, coutT, coutP, coutTechno) {
    var listeTechs = $('#listeTechnologies');
    $('#listeTechnologies').bootstrapTable('append', {
        codeTech: codeTech, nomTech: nomTech, nivActuel: nivActuel, nivVoulu: nivVoulu, coutM: coutM, coutT: coutT, coutP: coutP, coutTechno: coutTechno, actions: "<input type='button' class='btn btn-danger' value='x' />"
    });

    listeTechs.find('tbody td .btn').on('click', function () { EnleverTechnologieListe(this); });

    CalcTotalTechnologiesListe();
}

function ExporterListeBatiments() {
    prompt("Copiez ce texte pour le conserver ou transmettre la commande détaillée.", JSON.stringify($('#listeBatiments').bootstrapTable('getData')));
}

function ExporterListeTechnologies() {
    prompt("Copiez ce texte pour le conserver.", JSON.stringify($('#listeTechnologies').bootstrapTable('getData')));
}

function ExporterListeVaisseaux() {
    prompt("Copiez ce texte pour le conserver.", JSON.stringify($('#listeVaisseaux').bootstrapTable('getData')));
}

function RemplirListePlansVaisseaux(element) {
    var val = localStorage.getItem("PlansVaisseaux");
    listePlansVaisseaux = listeVaisseauxBase;
    if ($.isArray(val)) {
        listePlansVaisseaux += JSON.parse(val);
    }
    //listePlansVaisseaux.sort(function (a, b) { return ((a.chassis < b.chassis) ? -1 : ((a.chassis > b.chassis) ? 1 : 0)) });
    listePlansVaisseaux.keySort({ chassis: "asc", faction: "asc", nom: "asc" });

    if (!(element instanceof jQuery))
    {
        element = $(element);
    }
    var ancienChassis = "";
    $.each(listePlansVaisseaux, function (key, value) {
        if (ancienChassis !== value.chassis) {
            element.append($('<optgroup>', { label: typesVaisseaux[value.chassis] }));
            ancienChassis = value.chassis;
}
        element.append($('<option>', { value: value.codeVaisseau, text: value.nom }));
        //console.log(typesVaisseaux[value.chassis]);
    });
}

function ImporterListeBatiments() {
    var listeBats = $('#listeBatiments');
    listeBatiments = JSON.parse(prompt("Coller ici le texte exporté."));
    if (listeBatiments !== null) {
        listeBats.bootstrapTable('load', listeBatiments);

        listeBats.find('tbody td .btn').on('click', function () { EnleverBatimentListe(this); });
        CalcTotalBatimentsListe();
    }
}

function ImporterListeTechnologies() {
    var listeTechs = $('#listeTechnologies');
    listeTechnologies = JSON.parse(prompt("Coller ici le texte exporté."));
    if (listeTechnologies !== null) {
        listeTechs.bootstrapTable('load', listeTechnologies);

        listeTechs.find('tbody td .btn').on('click', function () { EnleverTechnologieListe(this); });
        CalcTotalTechnologiesListe();
    }
}

function ImporterListeVaisseaux() {
    var listeVaiss = $('#listeVaisseaux');
    listeVaisseaux = JSON.parse(prompt("Coller ici le texte exporté."));
    if (listeVaisseaux !== null) {
        listeVaiss.bootstrapTable('load', listeVaisseaux);

        listeVaiss.find('tbody td .btn').on('click', function () { EnleverVaisseauListe(this); });
        CalcTotalVaisseauxListe();
    }
}

function ViderBatimentListe() {
    $("#listeBatiments").bootstrapTable('load', []);
    CalcTotalBatimentsListe();
}

function ViderTechnologieListe() {
    $("#listeTechnologies").bootstrapTable('load', []);
    CalcTotalTechnologiesListe();
}

function ViderVaisseauListe() {
    $("#listeVaisseaux").bootstrapTable('load', []);
    CalcTotalVaisseauxListe();
}

function EnleverBatimentListe(element) {
    var listeBats = $('#listeBatiments');
    var position = $(element).parentsUntil('tr').parent().data('index');
    listeBatiments = listeBats.bootstrapTable('getData');
    listeBatiments.splice(position, 1);

    listeBats.bootstrapTable('load', listeBatiments);
    listeBats.find('tbody td .btn').on('click', function () { EnleverBatimentListe(this); });
    CalcTotalBatimentsListe();
}

function EnleverTechnologieListe(element) {
    var listeTechs = $('#listeTechnologies');
    var position = $(element).parentsUntil('tr').parent().data('index');
    listeTechnologies = listeTechs.bootstrapTable('getData');
    listeTechnologies.splice(position, 1);

    listeTechs.bootstrapTable('load', listeTechnologies);
    listeTechs.find('tbody td .btn').on('click', function () { EnleverTechnologieListe(this); });
    CalcTotalTechnologiesListe();
}

function EnleverVaisseauListe(element) {
    var listeVaiss = $('#listeVaisseaux');
    var position = $(listeVaiss).parentsUntil('tr').parent().data('index');
    listeTechnologies = listeVaiss.bootstrapTable('getData');
    listeTechnologies.splice(position, 1);

    listeVaiss.bootstrapTable('load', listeTechnologies);
    listeVaiss.find('tbody td .btn').on('click', function () { EnleverVaisseauListe(this); });
    CalcTotalVaisseauxListe();
}

function ChargerBatiment(row) {
    $('#selectBatiment').val(row.codeBat);
    $('#nudActuel').val(row.nivActuel);
    $('#nudDesire').val(row.nivVoulu);
    $('#txtColonieBat').val(row.nomColo).change();
}

function ChargerTechnologie(row) {
    $('#selectTechnologie').val(row.codeTech);
    $('#nudActuelTech').val(row.nivActuel);
    $('#nudDesireTech').val(row.nivVoulu);
}

function ChargerVaisseau(row) {
    $('#selectVaisseau').val(row.codeVaiss);
    $('#nudNombreVaiss').val(row.nombre);
}


function CalcTotalBatimentsListe() {
    SaveToLocal();
    var listeBats = $('#listeBatiments');
    listeBatiments = listeBats.bootstrapTable('getData');

    var totalMetal = 0, totalTrit = 0, totalPP = 0, nbrModules = 0;
    for (var i = 0; i < listeBatiments.length; i++) {
        totalMetal += parseInt(listeBatiments[i].coutM);
        totalTrit += parseInt(listeBatiments[i].coutT);
        totalPP += parseInt(listeBatiments[i].coutP);
        nbrModules += parseInt(listeBatiments[i].coutMKII);
    }

    $('.totalBatsMetal strong').text(big_number(totalMetal));
    $('.totalBatsMetal').attr('title', formatNumber(totalMetal));

    $('.totalBatsTrit strong').text(big_number(totalTrit));
    $('.totalBatsTrit').attr('title', formatNumber(totalTrit));

    $('.totalBatsPP strong').text(big_number(totalPP));
    $('.totalBatsPP').attr('title', formatNumber(totalPP));

    $('.totalBatsModules strong').text(big_number(nbrModules));
    $('.totalBatsModules').attr('title', formatNumber(nbrModules));

}

function CalcTotalTechnologiesListe() {
    SaveToLocal();
    var listeTechs = $('#listeTechnologies');
    listeTechnologies = listeTechs.bootstrapTable('getData');

    var totalMetal = 0, totalTrit = 0, totalPP = 0, coutTechno = 0;
    for (var i = 0; i < listeTechnologies.length; i++) {
        totalMetal += parseInt(listeTechnologies[i].coutM);
        totalTrit += parseInt(listeTechnologies[i].coutT);
        totalPP += parseInt(listeTechnologies[i].coutP);
        coutTechno += parseInt(listeTechnologies[i].coutTechno);
    }

    $('.totalTechsMetal strong').text(big_number(totalMetal));
    $('.totalTechsMetal').attr('title', formatNumber(totalMetal));

    $('.totalTechsTrit strong').text(big_number(totalTrit));
    $('.totalTechsTrit').attr('title', formatNumber(totalTrit));

    $('.totalTechsPP strong').text(big_number(totalPP));
    $('.totalTechsPP').attr('title', formatNumber(totalPP));

    $('.totalTechsTechno strong').text(big_number(coutTechno));
    $('.totalTechsTechno').attr('title', formatNumber(coutTechno));

}

function CalcTotalVaisseauxListe() {
    SaveToLocal();
    var listeTechs = $('#listeVaisseaux');
    listeVaisseaux = listeTechs.bootstrapTable('getData');

    var totalMetal = 0, totalTrit = 0, totalPP = 0, coutTechno = 0;
    for (var i = 0; i < listeVaisseaux.length; i++) {
        totalMetal += parseInt(listeVaisseaux[i].coutM);
        totalTrit += parseInt(listeVaisseaux[i].coutT);
        totalPP += parseInt(listeVaisseaux[i].coutP);
        coutZRC += parseInt(listeVaisseaux[i].coutZRC);
    }

    $('.totalVaissMetal strong').text(big_number(totalMetal));
    $('.totalVaissMetal').attr('title', formatNumber(totalMetal));

    $('.totalVaissTrit strong').text(big_number(totalTrit));
    $('.totalVaissTrit').attr('title', formatNumber(totalTrit));

    $('.totalVaissPP strong').text(big_number(totalPP));
    $('.totalVaissPP').attr('title', formatNumber(totalPP));

    $('.totalVaissZRC strong').text(big_number(coutZRC));
    $('.totalVaissZRC').attr('title', formatNumber(coutZRC));

}

function VoirDetailsModules() {
    var tauxMet = $('#tauxMetal').NumBox('getRaw') / 1000000;
    var tauxTrit = $('#tauxTritium').NumBox('getRaw') / 1000000;
    var tauxPP = $('#tauxPP').NumBox('getRaw');
    var coutMKII = parseInt(unFormat($('.totalBatsModules').attr('title')));
    var coutModules = coutVaisseaux("mcmkii", coutMKII, false);
    var totalZRC = Math.floor(coutEquivZRC(coutModules.metal, coutModules.tritium, 0, coutModules.zrc, tauxMet, tauxTrit, tauxPP) * 1.10);
    coutModules = coutVaisseaux('mcmkii', coutMKII, $('#chkGouvAm').is(':checked'));
    var texteResume = '<div class="col-md-6">Nombre de modules : </div><div class="col-md-6">' + formatNumber(coutMKII) + '</div>' +
        '<div class="col-md-6">Coût métal : </div><div class="col-md-6">' + big_number(coutModules.metal) + '</div>' +
        '<div class="col-md-6">Coût tritium : </div><div class="col-md-6">' + big_number(coutModules.tritium) + '</div>' +
        '<div class="col-md-6">Coût population : </div><div class="col-md-6">' + big_number(coutModules.pop) + '</div>' +
        '<div class="col-md-6">Coût ZRC : </div><div class="col-md-6">' + big_number(coutModules.zrc) + '</div>' +
        '<div class="col-md-6">Mode vendre : </div><div class="col-md-6">' + big_number(totalZRC) + '</div>';
    $('#resumeCommande').html(texteResume);
    //Un résumé de la commande avec le coût total
}

